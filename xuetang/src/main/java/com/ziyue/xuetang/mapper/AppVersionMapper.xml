<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lami.kaipin.mapper.AppVersion">




	<select id="getAppVersion" resultType="map" parameterType="map">
	
	 <![CDATA[
		SELECT  * from app_version
		where 
		device_type=#{device_type} and app_class=#{app_class}

		]]>

	</select>










</mapper>